<view class="page">
  <view class="weui-cell weui-cell_access" wx:for="{{shopMes}}">
    <view class="weui-cell__bd">
      <view style="display: inline-block; vertical-align: middle">{{item.title}}</view>
    </view>
    <view class="weui-cell__ft">{{item.mes}}</view>
  </view>
  <view class="page-section page-section-gap">
    <map
      id="myMap"
      style="width: 100%; height: 300px;"
      latitude="{{latitude}}"
      longitude="{{longitude}}"
      markers="{{markers}}"
      covers="{{covers}}"
      show-location
    ></map>
  </view>
</view>