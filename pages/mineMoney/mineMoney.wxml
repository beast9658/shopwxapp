<view class="page">
  <view class="header">
    <view class="weui-cell weui-cell_access">
      <view class="weui-cell__bd">
        <view style="display: inline-block; vertical-align: middle">我的余额：{{scoreTotal}}</view>
      </view>
    </view>
  </view>
  <view class="list">
    <view class="weui-cell weui-cell_access" wx:for="{{list}}" wx:key="{{index}}">
      <view class="weui-cell__bd">
        <view style="display: inline-block; vertical-align: middle">{{item.title}}</view>
        <view style="color:#bababa;font-size:24rpx;">{{item.time}}</view>
      </view>
      <view class="weui-cell__ft" style="color:#ff3c25">{{item.score}}</view>
    </view>
  </view>
</view>